#show_command
  = link_to t('common.title.back'), :hyundai_card_used_histories

h2= t(".title")

= form_tag [:upload, :hyundai_card_used_histories], multipart: true, class: "tabular" do
  .box
    p
      = label_tag :file, t('.file')
      = text_field_tag :upload, params[:upload][:file].original_filename
    p
      = submit_tag t('.submit')

  br

h3
  = t('creditcards.preview.preview')
= render 'list', menu: false
